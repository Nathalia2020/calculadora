'use estrito' ;

módulo . exportações  =  ( função ( )  {

  var  Calculadora  =  {

    somar : function ( )  {
      var  soma  =  0 ,  i  =  indefinido ;
      para (  i = 0 ;  i  <  argumentos . comprimento ;  i ++  )
        soma  + =  argumentos [ i ] ;

      retornar  soma ;
    } ,

    subtrair : function ( numero1 ,  numero2 )  {
      retornar  numero1  -  numero2 ;
    } ,

    multiplicar : function ( )  {
      var  multiplicacao  =  argumentos [ 0 ] ,  i  =  indefinido ;
      para (  i = 1 ;  i  <  argumentos . comprimento ;  i ++  )
        multiplicacao  * =  argumentos [ i ] ;

      return  multiplicacao ;
    } ,

    dividir : function ( numero1 ,  numero2 )  {
      return  numero1  /  numero2 ;
    }

  } ;

  return  Calculadora ;

} ) ( ) ;
